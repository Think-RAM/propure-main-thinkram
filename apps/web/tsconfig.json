{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "target": "ES6",
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"],
      "@propure/db": ["../../packages/db/src"],
      "@propure/db/*": ["../../packages/db/src/*"],
      "@propure/shared": ["../../packages/shared/src"],
      "@propure/shared/*": ["../../packages/shared/src/*"],
      "@propure/ai": ["../../packages/ai/src"],
      "@propure/ai/*": ["../../packages/ai/src/*"],
      "@propure/geo": ["../../packages/geo/src"],
      "@propure/geo/*": ["../../packages/geo/src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
